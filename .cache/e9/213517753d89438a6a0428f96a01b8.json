":79,"column":735},"source":"components/va-haircut.js","original":{"line":126,"column":82}},{"generated":{"line":79,"column":748},"source":"components/va-haircut.js","original":{"line":111,"column":4}},{"generated":{"line":80,"column":0},"source":"components/va-haircut.js","original":{"line":129,"column":3}},{"generated":{"line":82,"column":0},"source":"components/va-haircut.js","original":{"line":7,"column":69}},{"generated":{"line":82,"column":1},"source":"components/va-haircut.js","original":{"line":7,"column":0}}],"sources":{"components/va-haircut.js":"import { LitElement, html, css } from '@polymer/lit-element'\r\nimport { render } from 'lit-html'\r\nimport {anchorRoute, gotoRoute} from './../Router'\r\nimport Auth from './../Auth'\r\nimport App from './../App'\r\n\r\ncustomElements.define('va-haircut', class Haircut extends LitElement {\r\n  constructor(){\r\n    super()    \r\n  }\r\n\r\n  static get properties(){\r\n    return {\r\n      name: {\r\n        type: String\r\n      },\r\n      description: {\r\n        type: String\r\n      },\r\n      price: {\r\n        type: String\r\n      },\r\n      user: {\r\n        type: Object\r\n      },\r\n      image: {\r\n        type: String\r\n      },\r\n      gender: {\r\n        type: String\r\n      },\r\n      length: {\r\n        type: String\r\n      }\r\n    }\r\n  }\r\n\r\n  firstUpdated(){\r\n    super.firstUpdated()\r\n  }\r\n//if inside a method of event handler add the bind(this) as below\r\nmoreInfoHandler(){\r\n    //- create sl-dialog\r\n    console.log('more info handler')\r\n    const dialogEL = document.createElement('sl-dialog')\r\n\r\n    // add class name\r\n    dialogEL.className = 'haircut-dialog'\r\n\r\n    //- sl-dialog content (Paste #2)\r\n    const dialogContent = html`\r\n        <style>\r\n            .wrap {\r\n                display: flex;\r\n            }\r\n            .image {\r\n                width: 50%;\r\n            }\r\n            .image img {\r\n                width: 100%;\r\n            }\r\n            .content {\r\n                padding-left: 1em;\r\n            }\r\n            .gender span,\r\n            .length span {\r\n                text-transform: uppercase;\r\n                font-weight: bold;\r\n            }\r\n            .price {\r\n                font-size: 1.5em;\r\n                color: var(--brand-color);\r\n            }\r\n        </style>\r\n        <div class=\"wrap\">\r\n\r\n            <div class=\"content\">\r\n                <h1>${this.name}</h1>\r\n                <p>${this.description}</p>\r\n                <p class=\"price\">$${this.price}</p>\r\n                <p class=\"gender\">Gender: <span>${this.gender}</span></p>\r\n                <p class=\"length\">Length: <span>${this.length}</span></p>\r\n\r\n                <sl-button @click=${this.addFavHandler.bind(this)}>\r\n                <sl-icon slot=\"prefix\" name=\"heart-fill\"></sl-icon>\r\n                Add to Favourites\r\n                </sl-button>\r\n            </div>\r\n        </div>\r\n      `\r\n    render(dialogContent, dialogEL)\r\n\r\n    //- append to document.body\r\n    document.body.append(dialogEL)\r\n\r\n    //- show sl-dialog\r\n    dialogEL.show()\r\n\r\n    //- on hide delete sl-dialog\r\n    dialogEL.addEventListener('sl-after-hide', () => {\r\n        dialogEL.remove()\r\n    })\r\n\r\n  }\r\n\r\n  addFavHandler(){\r\n    alert(\"Add to Favourites\")\r\n  }\r\n\r\n  render(){    \r\n    return html`\r\n    <style>\r\n        .author {\r\n            font-size: 0.9em;\r\n            font-style: italic;\r\n            opacity: 0.8;\r\n        }\r\n    </style>\r\n\r\n    <sl-card>\r\n        <img slot=\"image\" src=\"${App.apiBase}/images/${this.image}\" />\r\n        <h2>${this.name}</h2>\r\n        <h3>$${this.price}</h3>\r\n        <p class=\"author\" >By ${this.user.firstName} ${this.user.lastName}</p>\r\n        <sl-button @click=${this.moreInfoHandler.bind(this)}>More Info</sl-button>\r\n        <sl-icon-button name=\"heart-fill\" label=\"Add to Favourites\" @click=${this.addFavHandler}></sl-icon-button> \r\n    </sl-card>\r\n   `\r\n  }\r\n  \r\n})\r\n"},"lineCount":null}},"error":null,"hash":"81c16ac13d2a5751956144d0dc12490d","cacheData":{"env":{}}}