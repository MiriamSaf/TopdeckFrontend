riginal":{"line":169,"column":20},"generated":{"line":1,"column":3963}},{"source":"components/td-packages.js","name":"vibe","original":{"line":169,"column":25},"generated":{"line":1,"column":3968}},{"source":"components/td-packages.js","original":{"line":174,"column":24},"generated":{"line":1,"column":3973}},{"source":"components/td-packages.js","name":"title","original":{"line":174,"column":29},"generated":{"line":1,"column":3978}},{"source":"components/td-packages.js","original":{"line":175,"column":19},"generated":{"line":1,"column":3984}},{"source":"components/td-packages.js","name":"country","original":{"line":175,"column":24},"generated":{"line":1,"column":3989}},{"source":"components/td-packages.js","original":{"line":176,"column":16},"generated":{"line":1,"column":3997}},{"source":"components/td-packages.js","name":"duration","original":{"line":176,"column":21},"generated":{"line":1,"column":4002}},{"source":"components/td-packages.js","original":{"line":176,"column":42},"generated":{"line":1,"column":4011}},{"source":"components/td-packages.js","name":"depatureLocation","original":{"line":176,"column":47},"generated":{"line":1,"column":4016}},{"source":"components/td-packages.js","original":{"line":177,"column":15},"generated":{"line":1,"column":4033}},{"source":"components/td-packages.js","name":"description","original":{"line":177,"column":20},"generated":{"line":1,"column":4038}},{"source":"components/td-packages.js","original":{"line":178,"column":79},"generated":{"line":1,"column":4050}},{"source":"components/td-packages.js","name":"addFavHandler","original":{"line":178,"column":84},"generated":{"line":1,"column":4055}}],"sources":{"components/td-packages.js":"import { LitElement, html, css } from '@polymer/lit-element'\r\nimport { render } from 'lit-html'\r\nimport {anchorRoute, gotoRoute} from '../Router'\r\nimport Auth from '../Auth'\r\nimport App from '../App'\r\n\r\n\r\n\r\ncustomElements.define('td-packages', class Package extends LitElement {\r\n  constructor(){\r\n    super()    \r\n  }\r\n\r\n  static get properties(){\r\n    return {\r\n      title: { type: String },\r\n      description: { type: String },\r\n      longDescription: { type: String },\r\n      date: { type: Date },\r\n      duration: { type: Number },\r\n      vibe: { type: String },\r\n      country: { type: String },\r\n      region: { type: String },\r\n      type: { type: String },\r\n      depatureLocation: { type: String },\r\n      endLocation: { type: String },\r\n      price: { type: Number },\r\n      image: { type: String }\r\n    }\r\n  }\r\n\r\n  firstUpdated(){\r\n    super.firstUpdated()\r\n  }\r\n//if inside a method of event handler add the bind(this) as below\r\nmoreInfoHandler(){\r\n    //- create sl-dialog\r\n    console.log('more info handler')\r\n    const dialogEL = document.createElement('sl-dialog')\r\n\r\n    // add class name\r\n    dialogEL.className = 'package-dialog'\r\n\r\n    //- sl-dialog content (Paste #2)\r\n    const dialogContent = html`\r\n        <style>\r\n            .wrap {\r\n                display: flex;\r\n            }\r\n            .image {\r\n                width: 50%;\r\n            }\r\n            .image img {\r\n                width: 100%;\r\n            }\r\n            .content {\r\n                padding-left: 1em;\r\n            }\r\n            .gender span,\r\n            .length span {\r\n                text-transform: uppercase;\r\n                font-weight: bold;\r\n            }\r\n            .price {\r\n                font-size: 1.5em;\r\n                color: var(--brand-color);\r\n            }\r\n        </style>\r\n        <div class=\"wrap\">\r\n\r\n            <div class=\"content\">\r\n                <h1>${this.title}</h1>\r\n                <p>${this.description}</p>\r\n                <p class=\"price\">$${this.price}</p>\r\n\r\n                <sl-button @click=${this.addFavHandler.bind(this)}>\r\n                  <sl-icon slot=\"prefix\" name=\"cart3\"></sl-icon>\r\n                  Add to Cart\r\n                </sl-button>\r\n            </div>\r\n        </div>\r\n      `\r\n    render(dialogContent, dialogEL)\r\n\r\n    //- append to document.body\r\n    document.body.append(dialogEL)\r\n\r\n    //- show sl-dialog\r\n    dialogEL.show()\r\n\r\n    //- on hide delete sl-dialog\r\n    dialogEL.addEventListener('sl-after-hide', () => {\r\n        dialogEL.remove()\r\n    })\r\n\r\n  }\r\n\r\n  addFavHandler(){\r\n    alert(\"Add to Favourites\")\r\n  }\r\n\r\n  render(){    \r\n    return html`\r\n    <style>\r\n      .packages-display{\r\n          display: flex;\r\n          flex-direction: row;\r\n          padding-bottom: 5vh;\r\n        }\r\n\r\n      .pckg-span{\r\n          border-radius: 8px;\r\n          text-align: center;\r\n          width: 20vh;\r\n          padding-top: 1vh;\r\n          padding-bottom: 1vh;\r\n          top: 1vh;\r\n          position: relative;\r\n          color: #FFF;\r\n          background-color: #C9E2D1;\r\n          margin-top: 20vh;\r\n        }\r\n\r\n\r\n\r\n      .pckg-img {\r\n          background-image: url(\"${App.apiBase}/images/${this.image}\");\r\n        display: flex;\r\n          justify-content: center;\r\n          align-items: center;    \r\n        background-position: center; \r\n        background-repeat: no-repeat;\r\n        background-size: cover;\r\n        height: 32vh;\r\n        width: 48vh;\r\n        border-radius: 10px;\r\n        margin-right: 4vh;\r\n        }\r\n\r\n\r\n      .pckg-disp-txt{\r\n          margin-top: 2vh;\r\n        }\r\n\r\n        .pckg-disp-txt h1{\r\n          color: var( --base-txt-color);\r\n          text-align: left;\r\n        }\r\n\r\n        .pckg-disp-txt h2{\r\n          font-size: 3vh;\r\n          margin-top: -2vh;\r\n        }\r\n\r\n        .pckg-disp-txt p{\r\n          font-size: 2vh;\r\n          margin-top: -1vh;\r\n  }\r\n\r\n        \r\n    </style>\r\n\r\n    \r\n\r\n      <div class = \"packages-display\">\r\n\r\n        <div class = \"pckg-img\">\r\n          <div class = \"pckg-span\" @click=${this.moreInfoHandler.bind(this)}>\r\n            <span>${this.vibe}</span>\r\n          </div>\r\n        </div>\r\n\r\n        <div class = \"pckg-disp-txt\">\r\n          <h1><strong>${this.title}</strong></h1>\r\n          <h2><i>${this.country}</i></h2>\r\n          <h1>${this.duration} Nights | ${this.depatureLocation}</h1>\r\n          <p>${this.description}</p>\r\n          <sl-icon-button name=\"heart-fill\" label=\"Add to Favourites\" @click=${this.addFavHandler}></sl-icon-button> \r\n        </div>\r\n\r\n      </div>\r\n\r\n    \r\n   `\r\n  }\r\n  \r\n})\r\n"},"lineCount":null}},"error":null,"hash":"7f596a0d59f9a2d860d2408d0758f641","cacheData":{"env":{}}}