,"original":{"line":101,"column":4}},{"generated":{"line":109,"column":0},"source":"UserAPI.js","original":{"line":102,"column":4}},{"name":"data","generated":{"line":109,"column":11},"source":"UserAPI.js","original":{"line":102,"column":11}},{"generated":{"line":109,"column":15},"source":"UserAPI.js","original":{"line":102,"column":4}},{"generated":{"line":110,"column":0},"source":"UserAPI.js","original":{"line":103,"column":3}},{"generated":{"line":112,"column":0},"source":"UserAPI.js","original":{"line":5,"column":14}},{"generated":{"line":114,"column":15},"source":"UserAPI.js","original":{"line":107,"column":15}},{"name":"UserAPI","generated":{"line":114,"column":19},"source":"UserAPI.js","original":{"line":107,"column":19}},{"generated":{"line":114,"column":26},"source":"UserAPI.js","original":{"line":107,"column":15}}],"sources":{"UserAPI.js":"import App from './App'\nimport Auth from './Auth'\nimport Toast from './Toast'\n\nclass UserAPI {\n   // userData if for a form only\n /* \n  async updateUser(userId, userData){\n    // validate\n    if(!userId || !userData) return\n    \n    // make fetch request to backend\n    const response = await fetch(`${App.apiBase}/user/${userId}`, {\n      method: \"PUT\",\n        headers: { \"Authorization\": `Bearer ${localStorage.accessToken}`},\n        body: userData\n    })\n\n    // if response not ok\n    if(!response.ok){\n      // console log error\n      const err = await response.json()\n      if(err) console.log(err)\n      // throw error (exit this function)      \n      throw new Error('Problem updating user')\n    }\n\n    // convert response payload into json - store as data\n    const data = await response.json()\n    \n    // return data\n    return data\n  } */\n\n  async getUser(userId){\n    // validate\n    if(!userId) return\n    \n    // fetch the json data\n    const response = await fetch(`${App.apiBase}/user/${userId}`, {\n      headers: { \"Authorization\": `Bearer ${localStorage.accessToken}`}\n    })\n\n    // if response not ok\n    if(!response.ok){ \n      // console log error\n      const err = await response.json()\n      if(err) console.log(err)\n      // throw error (exit this function)      \n      throw new Error('Problem getting user')\n    }\n    \n    // convert response payload into json - store as data\n    const data = await response.json()\n    \n    // return data\n    return data\n  }\n\n  \n  //Insert code 8a paste 2 = for both form data and json data\n  async updateUser(userId, userData, dataType = 'form'){\n    // validate\n    if(!userId || !userData) return\n    \n    let responseHeader\n    \n    // form data\n    if(dataType == 'form'){\n      // fetch response header normal (form data)\n      responseHeader = {\n        method: \"PUT\",        \n        headers: { \"Authorization\": `Bearer ${localStorage.accessToken}`},\n        body: userData\n      }\n      \n    // json data\n    }else if(dataType == 'json'){\n      responseHeader = {\n        method: \"PUT\",        \n        headers: { \"Authorization\": `Bearer ${localStorage.accessToken}`, \"Content-Type\" : \"application/json\"},\n        body: JSON.stringify(userData)\n      }\n    }\n  \n    // make fetch request to backend\n    const response = await fetch(`${App.apiBase}/user/${userId}`, responseHeader)\n  \n    // if response not ok\n    if(!response.ok){\n      // console log error\n      const err = await response.json()\n      if(err) console.log(err)\n      // throw error (exit this function)      \n      throw new Error('Problem updating user')\n    }\n  \n    // convert response payload into json - store as data\n    const data = await response.json()\n    \n    // return data\n    return data\n  }\n\n}\n\nexport default new UserAPI()"},"lineCount":null}},"error":null,"hash":"29d7935dbd5812712b0f8111c8db925d","cacheData":{"env":{}}}