'\r\nimport {anchorRoute, gotoRoute} from '../Router'\r\nimport Auth from '../Auth'\r\nimport App from '../App'\r\n\r\n\r\n\r\ncustomElements.define('td-packages', class Package extends LitElement {\r\n  constructor(){\r\n    super()    \r\n  }\r\n\r\n  static get properties(){\r\n    return {\r\n      title: { type: String },\r\n      description: { type: String },\r\n      longDescription: { type: String },\r\n      date: { type: Date },\r\n      duration: { type: Number },\r\n      vibe: { type: String },\r\n      country: { type: String },\r\n      region: { type: String },\r\n      type: { type: String },\r\n      depatureLocation: { type: String },\r\n      endLocation: { type: String },\r\n      price: { type: Number },\r\n      image: { type: String }\r\n    }\r\n  }\r\n\r\n  firstUpdated(){\r\n    super.firstUpdated()\r\n  }\r\n//if inside a method of event handler add the bind(this) as below\r\nmoreInfoHandler(){\r\n    //- create sl-dialog\r\n    console.log('more info handler')\r\n    const dialogEL = document.createElement('sl-dialog')\r\n\r\n    // add class name\r\n    dialogEL.className = 'package-dialog'\r\n\r\n    //- sl-dialog content (Paste #2)\r\n    const dialogContent = html`\r\n        <style>\r\n            .wrap {\r\n                display: flex;\r\n            }\r\n            .image {\r\n                width: 50%;\r\n            }\r\n            .image img {\r\n                width: 100%;\r\n            }\r\n            .content {\r\n                padding-left: 1em;\r\n            }\r\n            .gender span,\r\n            .length span {\r\n                text-transform: uppercase;\r\n                font-weight: bold;\r\n            }\r\n            .price {\r\n                font-size: 1.5em;\r\n                color: var(--brand-color);\r\n            }\r\n        </style>\r\n        <div class=\"wrap\">\r\n\r\n            <div class=\"content\">\r\n                <h1>${this.title}</h1>\r\n                <p>${this.description}</p>\r\n                <p class=\"price\">$${this.price}</p>\r\n\r\n                <sl-button @click=${this.addFavHandler.bind(this)}>\r\n                  <sl-icon slot=\"prefix\" name=\"cart3\"></sl-icon>\r\n                  Add to Cart\r\n                </sl-button>\r\n            </div>\r\n        </div>\r\n      `\r\n    render(dialogContent, dialogEL)\r\n\r\n    //- append to document.body\r\n    document.body.append(dialogEL)\r\n\r\n    //- show sl-dialog\r\n    dialogEL.show()\r\n\r\n    //- on hide delete sl-dialog\r\n    dialogEL.addEventListener('sl-after-hide', () => {\r\n        dialogEL.remove()\r\n    })\r\n\r\n  }\r\n\r\n  addFavHandler(){\r\n    alert(\"Add to Favourites\")\r\n  }\r\n\r\n  render(){    \r\n    return html`\r\n    <style>\r\n\r\n\r\n        \r\n    </style>\r\n\r\n    <sl-card>\r\n\r\n\r\n\r\n\r\n\r\n        <img src=\"${App.apiBase}/images/${this.image}\" width=\"130px\"/>\r\n        <h2>${this.title}</h2>\r\n        <h3>$${this.price}</h3>\r\n        <sl-button @click=${this.moreInfoHandler.bind(this)}>More Info</sl-button>\r\n        <sl-icon-button name=\"heart-fill\" label=\"Add to Favourites\" @click=${this.addFavHandler}></sl-icon-button> \r\n    </sl-card>\r\n   `\r\n  }\r\n  \r\n})\r\n"},"lineCount":null}},"error":null,"hash":"e8a75db4d3c65cdd2952fc23ef3ba2d9","cacheData":{"env":{}}}