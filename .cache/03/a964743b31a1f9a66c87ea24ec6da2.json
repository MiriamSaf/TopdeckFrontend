r-txt-color: #000;\n  --app-header-title-font-size: 1.6em;\n\n  // shoelace tokens\n  // full list here:\n  // https://github.com/shoelace-style/shoelace/blob/current/src/styles/base.scss\n  --sl-color-primary-50: #f4f4f4;\n  --sl-color-primary-100: #{lighten($brand-color, 40%)};\n  --sl-color-primary-200: #{lighten($brand-color, 30%)};\n  --sl-color-primary-300: #{lighten($brand-color, 20%)};\n  --sl-color-primary-400: #{lighten($brand-color, 10%)};\n  --sl-color-primary-500: #{$brand-color};\n  --sl-color-primary-600: #{darken($brand-color, 10%)};\n  --sl-color-primary-700: #{darken($brand-color, 20%)};\n  --sl-color-primary-800: #{darken($brand-color, 30%)};\n  --sl-color-primary-900: #{darken($brand-color, 40%)};\n  --sl-focus-ring-color-primary: #{rgba($brand-color, 0.1)};\n}","scss\\_base.scss":"// Base\n\n* {\n  box-sizing: border-box;\n  -webkit-tap-highlight-color:  rgba(255, 255, 255, 0);\n}\n\nbody {\n  padding: 0;  \n  margin: 0;\n  color: var(--base-txt-color);\n  font-family: var(--base-font-family);\n  font-weight: 300;\n  background: var(--body-bg);\n  height: 100vh;\n}\n\n\n// headings ------------\nh1,h2,h3,h5,h6 {\n  margin: 0 0 .5em;\n  color: var(--heading-color);\n} \n* {\n  margin: 0;\n  padding: 0;\n}\n\nbody{\n  font-family: 'Open Sans', 'Times New Roman', Times, serif;\n  font-weight: 700;\n}\n\nh1, h2, h3,h4,h5{\n  font-family: 'Roboto Condensed', 'Open Sans';    \n  font-weight: 300;\n}\n\n#home-hero-section{\n  background-image: url(\"./../../static/images/HOME-1.jpg\");\n  background-position: center; \n  background-repeat: no-repeat;\n  background-size: cover;\n  height: 100vh;\n}\n\n#home-hero-text h3,h1{\n  text-align: center;\n  font-weight:normal;\n  color: #FFF;\n}\n\n#home-hero-text{\n  margin-top: 45vh;\n  z-index: 99;\n}\n\n#home-hero-text h3{\n  font-size: 6vh;\n  padding-bottom: 2.8vh;\n  text-shadow: rgba(50, 50, 93, 0.25) 0px 50px 100px -20px, rgba(0, 0, 0, 0.3) 0px 30px 60px -30px, rgba(10, 37, 64, 0.35) 0px -2px 6px 0px inset;;\n}\n#home-hero-text h1{\n  font-size: 16vh;\n  text-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;;\n}\n\n// links ---------------\na {\n  color: var(--link-color);\n}\n\n\n// app splash screen -------\n.app-splash {\n  position: fixed;\n  background: #fff;\n  color: #777;\n  display: flex;\n  left: 0;\n  top:0;\n  right: 0;\n  bottom: 0;\n  align-items: center;\n  justify-content: center;\n  text-align: center;\n  .app-logo {\n    width: 180px;\n    display: block;\n    margin-bottom: 1em;\n  }\n}\n\n\n// helper classes ---------\n.calign {\n  text-align: center;\n}\n\n.large-text {\n  font-size: 1.6em;\n}\n\n.brand-color {\n  color: var(--brand-color);\n}\n\n// generic-entry ------------\n.generic-entry {\n  box-shadow: 0px 3px 5px rgba(0,0,0,0.07);\n  border-radius: 5px;\n  margin-bottom: .5em;\n  padding: 1em;\n  background: #fff;\n}\n\n// root ---------------\n#root {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;  \n  overflow: hidden;\n}\n\n// page --------------------\n.page-content {\n  position: absolute;\n  top: var(--app-header-height);\n  left: 0;\n  right: 0;\n  bottom: 0;\n  overflow-y: scroll;\n  padding: 1em;\n  &.no-pad {\n    padding: 0;\n  }\n}\n\n.page-centered {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n// sign in + sign up pages ----------\n.signinup-box,\n.signup-box {  \n  max-width: 320px;\n  text-align: center;\n  h1 {\n    color: black;\n  }\n}\n\n.signinup-logo {\n  width: 200px;\n  margin-bottom: 1em;\n}\n\n// guide page ---------------\n.guide-step {\n  padding: 1em;\n  margin: 1.5em auto;\n  max-width: 600px;\n  img {\n    width: 100%;\n    height: auto;\n  }\n}\n\n.packages-grid{\n  display: flex;\n  flex-wrap: wrap;\n  .package-card {\n    width: calc(33.3% - 1em);\n    margin: .5em;\n  }\n}\n\n// packages dialog\n.package-dialog {\n  --width: 70%;\n}\n\n.package-dialog::part(body){\n  margin-top: -3em;\n}\n\n\n// RESPONSIVE - MOBILE -------------------\n@media all and (max-width: 768px){ \n\n  .signinup-box {\n    width: 90%;\n    max-width: none;\n  }\n}","scss\\_forms.scss":"// Forms\n\n.input-group {\n  margin-bottom: .5em;\n}\n.page-form {\n  max-width: 700px;\n}","scss\\_shoelace.scss":"// Shoelace\n\n// sl-dialog\nsl-dialog::part(title){\n  font-weight: 500;\n  font-size: 1.6em;\n}\n\n\n// RESPONSIVE - MOBILE -------------------\n@media all and (max-width: 768px){ \n\n  sl-dialog::part(panel) {\n    width: 100%;\n    height: 100%;\n    max-height: none;\n    max-width: none;\n  }\n}","scss\\_toast.scss":"// Toast\n\n#toasts {\n  position: fixed;\n  left: 50%;  \n  top: 1em; \n  transform: translateX(-50%); \n  z-index: 999;\n  display: flex;\n  justify-content: center;\n  flex-direction: column; \n}\n\n.toast-entry {  \n  background:#fff;\n  margin: 0 auto;\n  color: #333;  \n  border-radius: 200px;\n  box-shadow: 0px 3px 25px rgba(0,0,0,0.14);\n  padding: .7em 1.3em;\n  font-size: 1em;\n  margin-bottom: .25em;\n  text-align: center;\n  transition: all .2 ease;\n  &.is-error {\n    background: rgb(212, 25, 93);\n    color:#fff;\n  }  \n}"},"lineCount":null}},"error":null,"hash":"a0599705bfc468169e82aae025d21fb4","cacheData":{"env":{}}}